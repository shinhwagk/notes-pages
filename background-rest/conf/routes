GET / controllers.Application.index

GET /api/labels         controllers.Application.allLabels

GET /api/label/:name    controllers.Application.getLabel(name:String)

GET /api/note/:id       controllers.Application.getNote(id:Int)

POST /api/note          controllers.Application.addNote

PUT /api/note/:id       controllers.Application.putNote(id:Int)

POST /api/label         controllers.Application.addLabel


